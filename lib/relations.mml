      <mspace linebreak="newline" />

      <mtable width="100%" columnspacing="0em 2em" columnalign="right left"><mtr><mtd><mrow><mi>S</mi><mn>3</mn></mrow></mtd><mtd><mrow><mo form="infix">=</mo><mn>120</mn></mrow></mtd></mtr><mtr><mtd><mrow><mi>Q</mi><mn>3</mn></mrow></mtd><mtd><mrow><mo form="infix">=</mo><mn>60</mn></mrow></mtd></mtr><mtr><mtd><mrow><mi>Q</mi><mn>1</mn><mo form="infix">+</mo><mi>S</mi><mn>1</mn></mrow></mtd><mtd><mrow><mo form="infix">=</mo><mn>60</mn></mrow></mtd></mtr></mtable>
      <mspace linebreak="newline" />

      <mtable width="100%" columnspacing="0em 2em" columnalign="right left"><mtr><mtd><mtable columnalign="center"><mtr><mtd><mrow><mi>s</mi><mi>i</mi><mi>n</mi><mo fence="true" stretchy="false">(</mo><mi>Q</mi><mn>1</mn><mo fence="true" stretchy="false">)</mo></mrow></mtd></mtr><mtr><mtd><mrow><mi>s</mi><mi>i</mi><mi>n</mi><mo fence="true" stretchy="false">(</mo><mi>S</mi><mn>2</mn><mo fence="true" stretchy="false">)</mo></mrow></mtd></mtr></mtable></mtd><mtd><mrow><mo form="infix">=</mo><mfrac><msqrt><mn>6</mn></msqrt><mn>4</mn></mfrac><mo form="infix">&#xB7;</mo><mfrac><mn>1</mn><mn>1</mn></mfrac><mspace is="true" width="0.22em"></mspace></mrow></mtd></mtr></mtable>
      <mspace linebreak="newline" />

      <mtable width="100%" columnspacing="0em 2em" columnalign="right left"><mtr><mtd><mrow><mi>s</mi><mi>i</mi><mi>n</mi><mo fence="true" stretchy="false">(</mo><mi>Q</mi><mn>2</mn><mo fence="true" stretchy="false">)</mo><mspace is="true" width="0.22em"></mspace></mrow></mtd><mtd><mrow><mo form="infix">=</mo><mfrac><msqrt><mn>6</mn></msqrt><mn>4</mn></mfrac><mo form="infix">&#xB7;</mo><mfrac><mi>&phi;</mi><mn>1</mn></mfrac></mrow></mtd></mtr></mtable>
      <mspace linebreak="newline" />

      <mtable width="100%" columnspacing="0em 2em" columnalign="right left"><mtr><mtd><mrow><mspace is="true" width="0.22em"></mspace><mi>s</mi><mi>i</mi><mi>n</mi><mo fence="true" stretchy="false">(</mo><mi>S</mi><mn>1</mn><mo fence="true" stretchy="false">)</mo><mspace is="true" width="0.22em"></mspace></mrow></mtd><mtd><mrow><mo form="infix">=</mo><mfrac><msqrt><mn>6</mn></msqrt><mn>4</mn></mfrac><mo form="infix">&#xB7;</mo><mfrac><mn>1</mn><mi>&phi;</mi></mfrac></mrow></mtd></mtr></mtable>
