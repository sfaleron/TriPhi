<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE math
  PUBLIC "-//W3C//DTD MathML 2.0//EN"
  "http://www.w3.org/Math/DTD/mathml2/mathml2.dtd">

<!-- This looks better after MathJax turns it into SVG than straight-up in Firefox! -->
<math xmlns:ns0="http://www.w3.org/1998/Math/MathML" display="block">
  <mpadded lspace="%HSHIFT%">
    <mtable>
      <mtr>
        <mtd>
          <menclose notation="bottom">
            <mstyle mathvariant="sans-serif">
              <mtext>Sides</mtext>
            </mstyle>
          </menclose>
        </mtd>
      </mtr>
      <mtr>
        <mtd>
          <mpadded lspace="%HSHIFT-BLK1%">
            <mtable columnalign="right left" columnspacing="0em 2em" width="100%">
              <mtr>
                <mtd>
                  <mrow>
                    <mi>q</mi>
                    <mn>1</mn>
                    <mo form="infix">=</mo>
                    <mi>s</mi>
                    <mn>1</mn>
                  </mrow>
                </mtd>
                <mtd>
                  <mrow>
                    <mo form="infix">=</mo>
                    <mn>1</mn>
                  </mrow>
                </mtd>
              </mtr>
              <mtr>
                <mtd>
                  <mrow>
                    <mi>q</mi>
                    <mn>2</mn>
                    <mo form="infix">=</mo>
                    <mi>s</mi>
                    <mn>2</mn>
                  </mrow>
                </mtd>
                <mtd>
                  <mrow>
                    <mo form="infix">=</mo>
                    <mi>φ</mi>
                  </mrow>
                </mtd>
              </mtr>
              <mtr>
                <mtd>
                  <mrow>
                    <mi>q</mi>
                    <mn>3</mn>
                  </mrow>
                </mtd>
                <mtd>
                  <mrow>
                    <mo form="infix">=</mo>
                    <msqrt>
                      <mn>2</mn>
                    </msqrt>
                  </mrow>
                </mtd>
              </mtr>
              <mtr>
                <mtd>
                  <mrow>
                    <mi>s</mi>
                    <mn>3</mn>
                  </mrow>
                </mtd>
                <mtd>
                  <mrow>
                    <mo form="infix">=</mo>
                    <msqrt>
                      <mn>2</mn>
                    </msqrt>
                    <mo form="infix">·</mo>
                    <mi>φ</mi>
                  </mrow>
                </mtd>
              </mtr>
            </mtable>
          </mpadded>
        </mtd>
      </mtr>
    <mtr /><mtr>
        <mtd>
          <menclose notation="bottom">
            <mstyle mathvariant="sans-serif">
              <mtext>Side Ratios</mtext>
            </mstyle>
          </menclose>
        </mtd>
      </mtr>
      <mtr>
        <mtd>
          <mfrac>
            <mstyle scriptlevel="-1">
              <mrow>
                <mi>s</mi>
                <mn>3</mn>
              </mrow>
            </mstyle>
            <mstyle scriptlevel="-1">
              <mrow>
                <mi>q</mi>
                <mn>3</mn>
              </mrow>
            </mstyle>
          </mfrac>
          <mo form="infix">=</mo>
          <mfrac>
            <mstyle scriptlevel="-1">
              <mrow>
                <mi>q</mi>
                <mn>2</mn>
              </mrow>
            </mstyle>
            <mstyle scriptlevel="-1">
              <mrow>
                <mi>q</mi>
                <mn>1</mn>
              </mrow>
            </mstyle>
          </mfrac>
          <mo form="infix">=</mo>
          <mfrac>
            <mstyle scriptlevel="-1">
              <mrow>
                <mi>s</mi>
                <mn>2</mn>
              </mrow>
            </mstyle>
            <mstyle scriptlevel="-1">
              <mrow>
                <mi>s</mi>
                <mn>1</mn>
              </mrow>
            </mstyle>
          </mfrac>
          <mo form="infix">=</mo>
          <mi>φ</mi>
        </mtd>
      </mtr>
      <mtr /><mtr>
        <mtd>
          <menclose notation="bottom">
            <mstyle mathvariant="sans-serif">
              <mtext>Height</mtext>
            </mstyle>
          </menclose>
        </mtd>
      </mtr>
      <mtr>
        <mtd>
          <mi>h</mi>
          <mo form="infix">=</mo>
          <mfrac>
            <mstyle scriptlevel="-1">
              <msqrt>
                <mn>3</mn>
              </msqrt>
            </mstyle>
            <mstyle scriptlevel="-1">
              <mn>2</mn>
            </mstyle>
          </mfrac>
          <mo form="infix">·</mo>
          <mi>φ</mi>
        </mtd>
      </mtr>
    </mtable>
  </mpadded>
</math>